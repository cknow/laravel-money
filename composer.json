{
    "name": "cknow/laravel-money",
    "description": "Laravel Money",
    "keywords": [
        "laravel",
        "money",
        "currency"
    ],
    "homepage": "https://github.com/cknow/laravel-money",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/cknow/laravel-money/issues",
        "source": "https://github.com/cknow/laravel-money"
    },
    "require": {
        "php": "^7.3.0",
        "ext-json": "*",
        "ext-intl": "*",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.5",
        "illuminate/filesystem": "^7.0|^8.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Cknow\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Database/Migrations/2020_04_30_000000_create_users_table.php"
        ],
        "psr-4": {
            "Cknow\\Money\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cknow\\Money\\MoneyServiceProvider"
            ]
        }
    }
}
