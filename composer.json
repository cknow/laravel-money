{
    "name": "cknow/laravel-money",
    "description": "Laravel Money",
    "keywords": [
        "laravel",
        "money",
        "currency"
    ],
    "homepage": "https://github.com/cknow/laravel-money",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-intl": "*",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "moneyphp/money": "^4.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Cknow\\Money\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cknow\\Money\\Tests\\": "tests/"
        },
        "files": [
            "tests/Database/Migrations/2020_04_30_000000_create_users_table.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cknow\\Money\\MoneyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}